<#compound> a api:API ;
        rdfs:label "Load Dataset"@en ;
        api:sparqlEndpoint <http://localhost:8890/sparql/> ;
        api:contentNegotiation api:parameterBased ;
        api:variable <#uri> ;
        api:variable <#graph> ;  
	api:vocabulary <http://rdfs.org/ns/void#> ;
	api:description "An API configuration to load data into the OpenPhacts System" ;
	api:endpoint [ 
		a api:TriggerLoadEndpoint ;
		api:name "Trigger Linkset Loading" ;
		api:description "Trigger the loading of queued linksets";
		api:uriTemplate "/triggerLinksetLoading" ;
		api:loadingCommand "/var/www/html/scripts/loadingScript.sh linksets";
		api:exampleRequestPath "/triggerLinksetLoading";
		] .

